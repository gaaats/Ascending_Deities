<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/bababab"
    android:layout_height="match_parent"
    tools:context=".Gaaame222Fragment">


    <ImageView
        android:id="@+id/img_elem_1"
        android:layout_width="120dp"
        android:contentDescription="ok"
        android:layout_height="120dp"
        android:src="@drawable/card"
        app:layout_constraintEnd_toEndOf="@id/njyhhynnhyjhy"
        app:layout_constraintBottom_toTopOf="@id/img_elem_2"
        app:layout_constraintStart_toStartOf="@id/hyjhyjhyhyjhy"
        app:layout_constraintTop_toTopOf="@id/hyyhhyhyhy" />

    <ImageView
        android:id="@+id/img_elem_1_back"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:padding="30dp"
        android:alpha="0"
        android:src="@drawable/dog1"
        app:layout_constraintEnd_toEndOf="@id/njyhhynnhyjhy"
        app:layout_constraintBottom_toTopOf="@id/img_elem_2"
        app:layout_constraintStart_toStartOf="@id/hyjhyjhyhyjhy"
        app:layout_constraintTop_toTopOf="@id/hyyhhyhyhy" />

    <ImageView
        android:id="@+id/img_elem_2"
        android:layout_width="120dp"
        android:contentDescription="ok"
        android:layout_height="120dp"
        android:src="@drawable/card"
        app:layout_constraintEnd_toEndOf="@id/njyhhynnhyjhy"
        app:layout_constraintStart_toStartOf="@id/hyjhyjhyhyjhy"
        app:layout_constraintTop_toBottomOf="@id/img_elem_1"
        app:layout_constraintBottom_toTopOf="@id/img_elem_3"/>

    <ImageView
        android:id="@+id/img_elem_2_back"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/dog1"
        android:alpha="0"
        android:padding="30dp"
        app:layout_constraintEnd_toEndOf="@id/njyhhynnhyjhy"
        app:layout_constraintStart_toStartOf="@id/hyjhyjhyhyjhy"
        app:layout_constraintTop_toBottomOf="@id/img_elem_1"
        app:layout_constraintBottom_toTopOf="@id/img_elem_3"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hyyhhyhyhy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hyjuujujuj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.95" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hyjuujujujuj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.85" />



    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hyhyjhyjihyiyh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hyjhyjhyhyjhy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/njyhhynnhyjhy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.50" />


    <ImageView
        android:id="@+id/img_elem_3"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/card"
        app:layout_constraintEnd_toEndOf="@id/njyhhynnhyjhy"
        app:layout_constraintStart_toStartOf="@id/hyjhyjhyhyjhy"
        app:layout_constraintTop_toBottomOf="@id/img_elem_2"
        app:layout_constraintBottom_toBottomOf="@id/hyjuujujujuj"/>

    <ImageView
        android:id="@+id/img_elem_3_back"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/dog1"
        android:alpha="0"
        android:padding="30dp"
        app:layout_constraintEnd_toEndOf="@id/njyhhynnhyjhy"
        app:layout_constraintStart_toStartOf="@id/hyjhyjhyhyjhy"
        app:layout_constraintTop_toBottomOf="@id/img_elem_2"
        app:layout_constraintBottom_toBottomOf="@id/hyjuujujujuj"/>


    <ImageView
        android:id="@+id/img_elem_6"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/card"
        app:layout_constraintEnd_toEndOf="@id/hyhyjhyjihyiyh"
        app:layout_constraintStart_toStartOf="@id/njyhhynnhyjhy"
        app:layout_constraintTop_toBottomOf="@id/img_elem_5"
        app:layout_constraintBottom_toBottomOf="@id/hyjuujujujuj"/>

    <ImageView
        android:id="@+id/img_elem_6_back"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/dog1"
        android:padding="30dp"
        android:alpha="0"
        app:layout_constraintEnd_toEndOf="@id/hyhyjhyjihyiyh"
        app:layout_constraintStart_toStartOf="@id/njyhhynnhyjhy"
        app:layout_constraintTop_toBottomOf="@id/img_elem_5"
        app:layout_constraintBottom_toBottomOf="@id/hyjuujujujuj"/>


    <ImageView
        android:id="@+id/img_main_element"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:src="@drawable/loolo"
        app:layout_constraintStart_toStartOf="@id/hyjhyjhyhyjhy"
        app:layout_constraintEnd_toEndOf="@id/hyhyjhyjihyiyh"
        app:layout_constraintBottom_toTopOf="@id/tv_dovn_text_gameeeee"
        app:layout_constraintTop_toBottomOf="@id/hyjuujujujuj"/>

    <TextView
        android:id="@+id/tv_dovn_text_gameeeee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="9dp"
        android:textColor="@color/white"
        android:alpha="0.7"
        android:fontFamily="@font/montserrat_black"
        android:text="find gold inside"
        android:textAllCaps="true"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/hyjuujujuj" />


    <ImageView
        android:id="@+id/img_elem_4"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/card"
        app:layout_constraintEnd_toEndOf="@id/hyhyjhyjihyiyh"
        app:layout_constraintBottom_toTopOf="@id/img_elem_5"
        app:layout_constraintStart_toStartOf="@id/njyhhynnhyjhy"
        app:layout_constraintTop_toTopOf="@id/hyyhhyhyhy" />

    <ImageView
        android:id="@+id/img_elem_4_back"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/dog1"
        android:padding="30dp"
        android:alpha="0"
        app:layout_constraintEnd_toEndOf="@id/hyhyjhyjihyiyh"
        app:layout_constraintBottom_toTopOf="@id/img_elem_5"
        app:layout_constraintStart_toStartOf="@id/njyhhynnhyjhy"
        app:layout_constraintTop_toTopOf="@id/hyyhhyhyhy" />

    <ImageView
        android:id="@+id/img_elem_5"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:contentDescription="ok"
        android:src="@drawable/card"
        app:layout_constraintEnd_toEndOf="@id/hyhyjhyjihyiyh"
        app:layout_constraintStart_toStartOf="@id/njyhhynnhyjhy"
        app:layout_constraintTop_toBottomOf="@id/img_elem_4"
        app:layout_constraintBottom_toTopOf="@id/img_elem_6"/>

    <ImageView
        android:id="@+id/img_elem_5_back"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/dog1"
        android:padding="30dp"
        android:alpha="0"
        app:layout_constraintEnd_toEndOf="@id/hyhyjhyjihyiyh"
        app:layout_constraintStart_toStartOf="@id/njyhhynnhyjhy"
        app:layout_constraintTop_toBottomOf="@id/img_elem_4"
        app:layout_constraintBottom_toTopOf="@id/img_elem_6"/>





</androidx.constraintlayout.widget.ConstraintLayout>