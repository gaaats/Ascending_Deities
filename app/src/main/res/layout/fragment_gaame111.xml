<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/bababab"
    android:layout_height="match_parent"
    tools:context=".veebviiiev.frrfrfrf.Gaame111Fragment">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hyjuujujuj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.95" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.50" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hyjhyjhyhyjhy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hyhyjhyjihyiyh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hyyhhyhyhy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />


    <ImageView
        android:id="@+id/img_titple_game"
        android:layout_width="220dp"
        android:layout_height="220dp"
        android:padding="12dp"
        android:scaleType="centerInside"
        android:src="@drawable/loolo"
        app:layout_constraintBottom_toBottomOf="@id/hyjuujujuj"
        app:layout_constraintEnd_toEndOf="@id/hyhyjhyjihyiyh"
        app:layout_constraintStart_toStartOf="@id/hyjhyjhyhyjhy"
        app:layout_constraintTop_toTopOf="@id/hyyhhyhyhy" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_fortune_machine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="26sp"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/montserrat_black"
        android:gravity="center"
        android:lines="1"
        android:text="@string/app_name"
        android:textAllCaps="true"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@id/hyhyjhyjihyiyh"
        app:layout_constraintStart_toStartOf="@id/hyjhyjhyhyjhy"
        app:layout_constraintBottom_toTopOf="@id/img_titple_game" />



    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/yhyhyyhyhyh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="24sp"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/montserrat_black"
        android:gravity="center"
        android:lines="1"
        android:padding="4dp"
        android:text="get super bonus x20"
        android:textAllCaps="true"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@id/hyhyjhyjihyiyh"
        android:layout_marginTop="16dp"
        android:background="@color/purple_500"
        app:layout_constraintStart_toStartOf="@id/hyjhyjhyhyjhy"
        app:layout_constraintTop_toBottomOf="@id/btn_play_game" />



    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_play_game"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_black"
        android:text="play game"
        android:layout_marginTop="32dp"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@id/hyhyjhyjihyiyh"
        app:layout_constraintStart_toStartOf="@id/hyjhyjhyhyjhy"
        app:layout_constraintTop_toBottomOf="@id/img_titple_game"
        app:layout_constraintVertical_chainStyle="packed" />




    <TextView
        android:id="@+id/tv_dovn_text_raitlist_fragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:alpha="0.7"
        android:fontFamily="@font/montserrat_black"
        android:text="@string/app_name"
        android:textAllCaps="true"
        android:textColor="@color/white"
        android:textSize="11sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/hyjuujujuj" />









</androidx.constraintlayout.widget.ConstraintLayout>